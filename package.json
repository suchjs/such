{
  "name": "suchjs",
  "version": "2.0.1",
  "description": "A fake data generator library, extendable and configable, mock data as you want easily.",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "author": "jxz_211@163.com",
  "keywords": [
    "faker",
    "fake",
    "data",
    "json",
    "generator",
    "generation",
    "simulate",
    "simulation",
    "mock",
    "mocking",
    "template",
    "intercept",
    "random",
    "nodejs",
    "typescript"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/eslint-parser": "^7.12.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "copy-webpack-plugin": "^9.0.1",
    "coveralls": "^3.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "filemanager-webpack-plugin": "^6.1.4",
    "husky": "^7.0.0",
    "jest": "^26.6.2",
    "madge": "^5.0.1",
    "prettier": "^2.1.2",
    "terser-webpack-plugin": "^5.1.4",
    "ts-jest": "^26.4.3",
    "ts-loader": "^9.2.3",
    "typescript": "^4.0.5",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2"
  },
  "scripts": {
    "start": "tsc && node lib/index.js",
    "test": "jest --forceExit",
    "lint": "eslint 'src/**/*.{ts,js}'",
    "check": "madge --warning --circular --extensions ts ./src/",
    "clear": "rm -rf ./lib/*",
    "build": "npm run clear && npm run lint && tsc",
    "build:web": "BROWSER=true webpack --progress --mode=\"production\"",
    "coverage": "jest --coverage && coveralls < coverage/lcov.info",
    "prepublish": "npm run build",
    "postpublish": "npm run clear",
    "prepare": "husky install"
  },
  "dependencies": {
    "nprintf": "^1.0.1",
    "reregexp": "^1.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/suchjs/such"
  },
  "bugs": {
    "url": "https://github.com/suchjs/such/issues"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
