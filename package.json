{
  "name": "suchjs",
  "version": "1.0.0-beta.1",
  "description": "A powerful javascript mocking library, written in typescript.",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "author": "jxz_211@163.com",
  "keywords": [
    "mock",
    "random",
    "javascript",
    "typescript",
    "string",
    "such",
    "suchjs"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/eslint-parser": "^7.12.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "coveralls": "^3.1.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^7.0.0",
    "jest": "^26.6.2",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.5",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2"
  },
  "scripts": {
    "start": "tsc && node lib/index.js",
    "test": "jest --forceExit",
    "lint": "eslint 'src/**/*.{ts,js}'",
    "clear": "rm -rf ./lib/*",
    "build": "npm run clear && npm run lint && tsc && npm run build:web",
    "build:web": "webpack --progress --mode=\"production\" && cp ./dist/such.$npm_package_version.min.js ./dist/such.min.js && npm run clear",
    "coverage": "jest --coverage && coveralls < coverage/lcov.info",
    "prepublish": "npm run build",
    "postpublish": "npm run clear",
    "prepare": "husky install"
  },
  "dependencies": {
    "nprintf": "^1.0.1",
    "reregexp": "^1.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/suchjs/such"
  },
  "bugs": {
    "url": "https://github.com/suchjs/such/issues"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
