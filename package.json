{
  "name": "suchjs",
  "version": "3.0.3",
  "description": "Generating fake data, define your own types easily, expandable and powerful.",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/exports/index.mjs"
    }
  },
  "author": "jxz_211@163.com",
  "keywords": [
    "fake",
    "generate",
    "mock",
    "random",
    "regex",
    "data",
    "json",
    "template",
    "string",
    "intercept",
    "xmlHttpRequest",
    "fetch",
    "browser"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "copy-webpack-plugin": "^9.0.1",
    "coveralls": "^3.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^8.3.0",
    "filemanager-webpack-plugin": "^6.1.4",
    "git-cz": "^4.8.0",
    "husky": "^7.0.0",
    "jest": "^27.4.7",
    "madge": "^5.0.1",
    "prettier": "^2.5.1",
    "terser-webpack-plugin": "^5.3.0",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1"
  },
  "scripts": {
    "start": "tsc && node lib/index.js",
    "test": "jest --forceExit",
    "lint": "eslint 'src/**/*.{ts,js}'",
    "check": "madge --warning --circular --extensions ts ./src/",
    "clear": "rm -rf ./lib/*",
    "build": "npm run clear && npm run lint && tsc && mkdir -p ./lib/exports && cp ./src/exports/index.mjs ./lib/exports/index.mjs",
    "build:web": "BROWSER=true webpack --progress --mode=\"production\"",
    "coverage": "jest --coverage && coveralls < coverage/lcov.info",
    "prepublish": "npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "nprintf": "^1.0.1",
    "reregexp": "^1.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/suchjs/such"
  },
  "bugs": {
    "url": "https://github.com/suchjs/such/issues"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
}
