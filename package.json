{
  "name": "suchjs",
  "version": "0.0.2",
  "description": "An easy to use and extend mock tool.",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "author": "ONEBOX FE",
  "keywords": ["suchjs","such","mock"],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "jest": "^22.4.3",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.3.0",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.6.0",
    "tslint": "^5.10.0",
    "typescript": "^3.1.6",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2"
  },
  "scripts": {
    "start": "tsc && node lib/such.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "local": "ts-node -r tsconfig-paths/register src/local/test.ts",
    "build:browser": "webpack --progress",
    "prepublish": "tsc"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/suchjs/such"
  },
  "bugs": {
    "url": "https://github.com/suchjs/such/issues"
  }
}
